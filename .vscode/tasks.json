{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Build SpeedTest.exe",
			"command": "cl.exe",
			"args": [
				"/Zi",
				"/EHsc",
				"/Fe:",
				"${fileDirname}\\SpeedTest.exe",
				"${workspaceFolder}\\*.cpp"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build"
		}
	]
} 